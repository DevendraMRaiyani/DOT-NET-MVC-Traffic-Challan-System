}
<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserEmail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ConfPassword)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AadharNo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserEmail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ConfPassword)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AadharNo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PublicUserId }) |
            @Html.ActionLink("Details", "Details", new { id=item.PublicUserId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PublicUserId })
        </td>
    </tr>
}

</table>
